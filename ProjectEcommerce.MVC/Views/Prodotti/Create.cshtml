@model ProdottoViewModel

<h1>Inserisci nuovo prodotto</h1>
<form method="post">

    <partial name="_Partial" model="Model" />

    <input type="submit" value="Salva" class="btn btn-warning" />
    <input type="reset" value="Reset" class="btn btn-success" />
    <a class="btn btn-primary" asp-controller="Prodotti" asp-action="Index" role="button">Indietro</a>
</form>
